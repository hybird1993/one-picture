{
  "name": "one-picture",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "dev:18": "vue-cli-service serve --mode dev18",
    "dev:20": "vue-cli-service serve --mode dev20",
    "dev:254": "vue-cli-service serve --mode dev254",
    "analyz": "vue-cli-service build --mode analyz--report"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "core-js": "^2.6.5",
    "echarts": "^4.2.1",
    "element-ui": "^2.11.1",
    "node-sass": "^7.0.0",
    "qs": "^6.8.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue": "^2.6.10",
    "x2js": "^3.3.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "compression-webpack-plugin": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "image-webpack-loader": "^6.0.0",
    "postcss-pxtorem": "^4.0.1",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0,
      "no-unused-vars": 0,
      "no-empty": 0,
      "vue/require-valid-default-prop": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-pxtorem": {
        "rootValue": 12,
        "unitPrecision": 5,
        "propList": [
          "*"
        ],
        "selectorBlackList": [],
        "mediaQuery": false,
        "minPixelValue": 0
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
